                    circle.setAttribute("cx", this.puntos[i].x);
                    circle.setAttribute("cy", this.puntos[i].y);
                    circle.setAttribute("r", 3);
                    circle.setAttribute("fill", "red");
                    svgElement.appendChild(circle);
                }
            }

            // Funci=C3=B3n para calcular el centroide de los puntos
            calcularCentroide() {
                let sumaX =3D 0;
                let sumaY =3D 0;

                for (let punto of this.puntos) {
                    sumaX +=3D punto.x;
                    sumaY +=3D punto.y;
                }

                return new Punto(sumaX / this.puntos.length, sumaY / this.p=
untos.length);
            }

            // Funci=C3=B3n para ordenar los puntos en sentido horario
            ordenarPuntosHorario() {
                const centroide =3D this.calcularCentroide();
                this.puntos.sort((a, b) =3D&gt; {
                    const anguloA =3D Math.atan2(a.y - centroide.y, a.x - c=
entroide.x);
                    const anguloB =3D Math.atan2(b.y - centroide.y, b.x - c=
entroide.x);
                    return anguloA - anguloB;
