    &lt;script&gt;
        // Clase Punto con encapsulamiento
        class Punto {
            #x; // Coordenada X privada
            #y; // Coordenada Y privada
           =20
            constructor(x, y) {
                this.#x =3D x;
                this.#y =3D y;
            }

            get x() {
                return this.#x;
            }

            get y() {
                return this.#y;
            }
        }

        // Clase Figura para manejar una lista de puntos y trazar la figura
        class Figura {
            constructor(puntos =3D []) {
                this.puntos =3D puntos;
            }

            // Funci=C3=B3n para agregar un nuevo punto a la figura
            agregarPunto(punto) {
                this.puntos.push(punto);
            }

            // Funci=C3=B3n para calcular el centroide de los puntos
            calcularCentroide() {
                let sumaX =3D 0;
                let sumaY =3D 0;

                for (let punto of this.puntos) {
                    sumaX +=3D punto.x;
                    sumaY +=3D punto.y;
                }

                return new Punto(sumaX / this.puntos.length, sumaY / this.p=
untos.length);
            }

            // Funci=C3=B3n para ordenar los puntos en sentido horario
            ordenarPuntosHorario() {
                const centroide =3D this.calcularCentroide();
                this.puntos.sort((a, b) =3D&gt; {
                    const anguloA =3D Math.atan2(a.y - centroide.y, a.x - c=
entroide.x);
                    const anguloB =3D Math.atan2(b.y - centroide.y, b.x - c=
entroide.x);
                    return anguloA - anguloB;
                });
            }

            // Funci=C3=B3n para vectorizar la figura en el canvas SVG
            dibujarContorno(svgElement) {
                const namespace =3D "http://www.w3.org/2000/svg";
                let path =3D document.createElementNS(namespace, "path");
                let d =3D this.generarCaminoVector(this.puntos);
               =20
                path.setAttribute("d", d);
                path.setAttribute("stroke", "black");
                path.setAttribute("fill", "none");
                svgElement.appendChild(path);
            }

            // Funci=C3=B3n que genera el camino SVG para el contorno del p=
ol=C3=ADgono
            generarCaminoVector(puntos) {
                let d =3D <code>M ${puntos[0].x} ${puntos[0].y}</code>; // =
Mueve al primer punto
                for (let i =3D 1; i &lt; puntos.length; i++) {
                    d +=3D <code> L ${puntos[i].x} ${puntos[i].y}</code>; /=
/ Traza l=C3=ADneas entre los puntos
                }
                d +=3D " Z"; // Cierra el pol=C3=ADgono
                return d;
            }

            // Funci=C3=B3n para dibujar los puntos como c=C3=ADrculos (ras=
terizaci=C3=B3n)
            dibujarPuntos(svgElement) {
                const namespace =3D "http://www.w3.org/2000/svg";
                for (let i =3D 0; i &lt; this.puntos.length; i++) {
                    let circle =3D document.createElementNS(namespace, "cir=
cle");
                    circle.setAttribute("cx", this.puntos[i].x);
                    circle.setAttribute("cy", this.puntos[i].y);
                    circle.setAttribute("r", 3);
                    circle.setAttribute("fill", "red");
                    svgElement.appendChild(circle);
                }
            }

            // Funci=C3=B3n para determinar si la figura es convexa o c=C3=
=B3ncava
            calcularConvexidad() {
                return this.esFiguraConvexa(this.puntos);
            }

            // Funci=C3=B3n para determinar si la figura es convexa o c=C3=
=B3ncava
            esFiguraConvexa(puntos) {
                let numPuntos =3D puntos.length;
                if (numPuntos &lt; 4) return true; // Un tri=C3=A1ngulo sie=
mpre es convexo

                let signos =3D [];
                for (let i =3D 0; i &lt; numPuntos; i++) {
                    let dx1 =3D puntos[(i + 2) % numPuntos].x - puntos[(i +=
 1) % numPuntos].x;
                    let dy1 =3D puntos[(i + 2) % numPuntos].y - puntos[(i +=
 1) % numPuntos].y;
                    let dx2 =3D puntos[i].x - puntos[(i + 1) % numPuntos].x=
;
                    let dy2 =3D puntos[i].y - puntos[(i + 1) % numPuntos].y=
;

                    let cruzProducto =3D dx1 * dy2 - dy1 * dx2;
                    signos.push(cruzProducto);
                }

                let positivos =3D signos.filter(s =3D&gt; s &gt; 0);
                let negativos =3D signos.filter(s =3D&gt; s &lt; 0);

                // Si todos los productos cruzados tienen el mismo signo, e=
s convexa
                return positivos.length =3D=3D=3D 0 || negativos.length =3D=
=3D=3D 0;
            }

            // Funci=C3=B3n para dibujar la figura completa
            render(svgElement) {
                this.ordenarPuntosHorario(); // Ordena los puntos antes de =
dibujar
                this.dibujarContorno(svgElement); // Dibuja el contorno
                this.dibujarPuntos(svgElement);   // Dibuja los puntos
            }
        }

