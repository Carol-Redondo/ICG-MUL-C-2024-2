    &lt;h1&gt;Rasterizaci=C3=B3n y Generaci=C3=B3n de Pol=C3=ADgonos&lt;/h1=
&gt;
    &lt;svg id=3D"canvas" width=3D"400" height=3D"400"&gt;&lt;/svg&gt;
    &lt;button id=3D"regenerarPuntosButton"&gt;Regenerar Puntos&lt;/button&=
gt; &lt;!-- Bot=C3=B3n para regenerar puntos --&gt;
    &lt;button id=3D"generarPoligonoButton"&gt;Generar Pol=C3=ADgono&lt;/bu=
tton&gt; &lt;!-- Bot=C3=B3n para generar pol=C3=ADgono --&gt;

    &lt;script&gt;
        // Clase Punto con encapsulamiento
        class Punto {
            #x; // Coordenada X privada
            #y; // Coordenada Y privada
           =20
            constructor(x, y) {
                this.#x =3D x;
                this.#y =3D y;
            }

            get x() {
                return this.#x;
            }

            get y() {
                return this.#y;
            }
        }

        // Clase Figura para manejar una lista de puntos y dibujar en el ca=
nvas
        class Figura {
            constructor(puntos =3D []) {
                this.puntos =3D puntos;
            }

            // Funci=C3=B3n para dibujar los puntos como c=C3=ADrculos (ras=
terizaci=C3=B3n)
            dibujarPuntos(svgElement) {
                const namespace =3D "http://www.w3.org/2000/svg";
                svgElement.innerHTML =3D ''; // Limpiar el SVG antes de red=
ibujar
                for (let i =3D 0; i &lt; this.puntos.length; i++) {
                    let circle =3D document.createElementNS(namespace, "cir=
cle");
                    circle.setAttribute("cx", this.puntos[i].x);
                    circle.setAttribute("cy", this.puntos[i].y);
                    circle.setAttribute("r", 3);
                    circle.setAttribute("fill", "red");
                    svgElement.appendChild(circle);
                }
            }

            // Funci=C3=B3n para dibujar el centroide
            dibujarCentroide(svgElement) {
                const centroide =3D this.calcularCentroide();
                const namespace =3D "http://www.w3.org/2000/svg";
                let circle =3D document.createElementNS(namespace, "circle"=
);
                circle.setAttribute("cx", centroide.x);
                circle.setAttribute("cy", centroide.y);
                circle.setAttribute("r", 5);
                circle.setAttribute("fill", "blue");
                svgElement.appendChild(circle);
            }

            // Funci=C3=B3n para calcular el centroide del pol=C3=ADgono
            calcularCentroide() {
                let centroideX =3D 0;
                let centroideY =3D 0;
                this.puntos.forEach(p =3D&gt; {
                    centroideX +=3D p.x;
                    centroideY +=3D p.y;
                });
                centroideX /=3D this.puntos.length;
                centroideY /=3D this.puntos.length;
                return new Punto(centroideX, centroideY);
            }

            // Funci=C3=B3n para dibujar la figura completa
            render(svgElement) {
                this.dibujarPuntos(svgElement);   // Dibuja los puntos
                this.dibujarCentroide(svgElement); // Dibuja el centroide
            }
        }

        // Funci=C3=B3n para generar puntos aleatorios
        function generarPuntosAleatorios(numPuntos, ancho, alto) {
            let puntosAleatorios =3D [];
            for (let i =3D 0; i &lt; numPuntos; i++) {
                let x =3D Math.random() * ancho; // Coordenada X aleatoria
                let y =3D Math.random() * alto;  // Coordenada Y aleatoria
                puntosAleatorios.push(new Punto(x, y));
            }
            return puntosAleatorios;
        }

        // Funci=C3=B3n para calcular el centroide de un conjunto de puntos
        function calcularCentroide(puntos) {
            let centroideX =3D 0;
            let centroideY =3D 0;
            puntos.forEach(p =3D&gt; {
                centroideX +=3D p.x;
                centroideY +=3D p.y;
            });
            centroideX /=3D puntos.length;
            centroideY /=3D puntos.length;
            return new Punto(centroideX, centroideY);
        }

        // Funci=C3=B3n para ordenar los puntos en sentido antihorario
        function ordenarPuntos(puntos) {
            const centroide =3D calcularCentroide(puntos);
            puntos.sort((a, b) =3D&gt; {
                return Math.atan2(a.y - centroide.y, a.x - centroide.x) - M=
ath.atan2(b.y - centroide.y, b.x - centroide.x);
            });
        }

        // Crear el canvas
        const svgCanvas =3D document.getElementById('canvas');

        // Generar una figura con puntos aleatorios
        const anchoCanvas =3D 400;
        const altoCanvas =3D 400;
        let figura =3D new Figura();

        // Evento para regenerar un nuevo conjunto de puntos al hacer clic =
en el bot=C3=B3n
        document.getElementById('regenerarPuntosButton').addEventListener('=
click', () =3D&gt; {
            figura =3D new Figura(generarPuntosAleatorios(10, anchoCanvas, =
altoCanvas));
            figura.render(svgCanvas);
        });

        // Evento para generar un pol=C3=ADgono al hacer clic en el bot=C3=
=B3n
        document.getElementById('generarPoligonoButton').addEventListener('=
click', () =3D&gt; {
            let puntosAleatorios =3D generarPuntosAleatorios(5, anchoCanvas=
, altoCanvas); // Generar 5 puntos para el pol=C3=ADgono
            ordenarPuntos(puntosAleatorios); // Ordenar los puntos
            figura =3D new Figura(puntosAleatorios);
