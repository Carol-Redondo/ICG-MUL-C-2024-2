        const regenerarButton =3D document.getElementById('regenerarButton'=
);

        // Crear la figura con una lista de puntos aleatorios
        function crearFiguraAleatoria() {
            let figura =3D new Figura();
            for (let i =3D 0; i &lt; 10; i++) {
