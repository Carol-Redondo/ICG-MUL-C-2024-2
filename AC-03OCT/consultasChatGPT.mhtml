    &lt;button id=3D"regenerarButton"&gt;Regenerar Puntos&lt;/button&gt;

    &lt;script&gt;
        // Clase Punto con encapsulamiento
        class Punto {
            #x; // Coordenada X privada
            #y; // Coordenada Y privada
           =20
            constructor(x, y) {
                this.#x =3D x;
                this.#y =3D y;
            }

            get x() {
                return this.#x;
            }

            get y() {
                return this.#y;
